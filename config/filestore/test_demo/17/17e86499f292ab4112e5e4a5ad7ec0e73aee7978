__exports.getFirstAccountFunction=getFirstAccountFunction;function getFirstAccountFunction(tokens){return getFunctionsFromTokens(tokens,["ODOO.BALANCE","ODOO.CREDIT","ODOO.DEBIT"])[0];}
return __exports;});;

