for(let day of weekend){if(day!=="0"&&day!=="1"){return false;}}
return true;},_t('When weekend is a string (%s) it must be composed of "0" or "1".',weekend));let result=[];for(let i=0;i<7;i++){if(weekend[i]==="1"){result.push((i+1)%7);}}
return result;}
