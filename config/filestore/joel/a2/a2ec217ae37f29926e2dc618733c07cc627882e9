return parts;}
function _backgroundImagePartsToCss(parts){let css=parts.url||'';if(parts.gradient){css+=(css?', ':'')+parts.gradient;}
return css||'none';}
function _isColorGradient(value){return value&&value.includes('-gradient(');}
