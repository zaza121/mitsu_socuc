odoo.define('@web_tour/tour_service/tour_step_automatic',['@web/core/browser/browser','@web/core/utils/ui','@web_tour/tour_service/tour_state','@odoo/hoot-dom','@web/../lib/hoot-dom/helpers/events','@web_tour/tour_service/tour_utils','@web_tour/tour_service/tour_helpers','@web_tour/tour_service/tour_step'],function(require){'use strict';let __exports={};const{browser}=require("@web/core/browser/browser");const{_legacyIsVisible}=require("@web/core/utils/ui");const{tourState}=require("@web_tour/tour_service/tour_state");const hoot=require("@odoo/hoot-dom");const{setupEventActions}=require("@web/../lib/hoot-dom/helpers/events");const{callWithUnloadCheck}=require("@web_tour/tour_service/tour_utils");const{TourHelpers}=require("@web_tour/tour_service/tour_helpers");const{TourStep}=require("@web_tour/tour_service/tour_step");const TourStepAutomatic=__exports.TourStepAutomatic=class TourStepAutomatic extends TourStep{triggerFound=false;hasRun=false;isBlocked=false;running=false;constructor(data,tour,index){super(data,tour);this.index=index;this.tourConfig=tourState.getCurrentConfig();}
