<odoo>

    <record id="view_equip_move_history_tree" model="ir.ui.view">
        <field name="name">opsol_costadoro.equip_move_history.tree</field>
        <field name="model">opsol_costadoro.equip_move_history</field>
        <field name="arch" type="xml">
            <list string="Historiques" default_order="create_date desc">
                <field name="create_date" column_invisible="1"/>
                <field name="date"/>
                <field name="user_id"/>
                <field name="equip_id"/>
                <field name="old_value"/>
                <field name="new_value"/>
            </list>
        </field>
    </record>

    <record id="action_equip_move_history" model="ir.actions.act_window">
        <field name="name">Historiques</field>
        <field name="res_model">opsol_costadoro.equip_move_history</field>
        <field name="view_mode">list</field>
        <field name="context">{}</field>
        <field name="help" type="html">
          <p>l historique de vos modification vont apparaitre ici.</p>
        </field>
    </record>

    <menuitem 
        id="menu_client_equipment_historique"
        name="Historiques"
        action="action_equip_move_history"
        parent="opsol_costadoro.menu_client_equipment_suivi"
        sequence="15" />

</odoo>