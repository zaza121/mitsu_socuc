<odoo>

	<record id="view_picking_form_hyd" model="ir.ui.view">
        <field name="name">stock.picking.form</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form" />
        <field name="arch" type="xml">
            
            <xpath expr="//field[@name='move_ids_without_package']/list[1]/field[@name='product_uom_qty']" position="after">
                <field name="in_store" optional="show" />
                <field name="aj_note" optional="show" />
            </xpath>

        </field>
    </record>

</odoo>